    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "module": "commonjs",
    "target": "ES2020",
    "jsx": "react",
    "baseUrl": ".",
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ]
}
